{
  "api": {
    "version": "v2",
    "base_url_template": "https://www.zohoapis.{dc}/bigin/v2",
    "data_centers": {
      "com": "United States",
      "eu": "Europe",
      "in": "India",
      "au": "Australia",
      "jp": "Japan",
      "uk": "United Kingdom",
      "ca": "Canada"
    }
  },
  "modules": {
    "Pipelines": {
      "description": "Core sales pipeline module (equivalent to Deals in Zoho CRM)",
      "api_name": "Pipelines",
      "ui_name": "Pipelines"
    },
    "Contacts": {
      "description": "Contact management",
      "api_name": "Contacts",
      "ui_name": "Contacts"
    },
    "Accounts": {
      "description": "Companies/Accounts management",
      "api_name": "Accounts",
      "ui_name": "Companies"
    },
    "Products": {
      "description": "Product catalog",
      "api_name": "Products",
      "ui_name": "Products"
    },
    "Tasks": {
      "description": "Task management",
      "api_name": "Tasks",
      "ui_name": "Tasks"
    },
    "Events": {
      "description": "Events and meetings",
      "api_name": "Events",
      "ui_name": "Events"
    },
    "Calls": {
      "description": "Call logging",
      "api_name": "Calls",
      "ui_name": "Calls"
    },
    "Notes": {
      "description": "Notes and attachments",
      "api_name": "Notes",
      "ui_name": "Notes"
    }
  },
  "pagination": {
    "default_limit": 200,
    "max_limit": 200
  },
  "rate_limits": {
    "requests_per_minute": 100,
    "bulk_api_requests_per_day": 10000
  }
